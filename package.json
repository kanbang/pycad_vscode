{
    "name": "pycad",
    "displayName": "pycad",
    "description": "script draw",
    "version": "0.0.1",
    "publisher": "kk",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runpycad"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.runpycad",
                "title": "Run"
            }
        ],
        "menus": {
                "editor/title": [
                    {
                        "command": "extension.runpycad",
                        "when": "editorLangId == python",
                        "group": "navigation"
                    }
          ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
